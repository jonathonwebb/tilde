{
    "compilerOptions": {
        "lib": ["dom", "dom.iterable", "es2020"],
        "target": "es2020",
        "module": "esnext",
        "moduleResolution": "node",
        "isolatedModules": true,
        "noEmit": true,
        "strict": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "jsx": "react",
        "jsxFactory": "h",
        "jsxFragmentFactory": "Fragment"
    }
}
